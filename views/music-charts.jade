    script(type='text/javascript').
      $.getJSON("today.json", function(json) {
      document.getElementById("cover").src = json[0].cover;
      document.getElementById("title").innerHTML = json[0].title;
      document.getElementById("artist").innerHTML = json[0].artist;
      document.getElementById("weeks-on-chart").innerHTML = json[0].position.weeksOnChart + " weeks on chart";
      document.getElementById("top-100-cover").src = json[0].cover;
      document.getElementById("top-100-title").innerHTML = json[0].title;
      document.getElementById("top-100-artist").innerHTML = json[0].artist;
      document.getElementById("100-weeks-on-chart").innerHTML = json[0].position.weeksOnChart + " weeks on chart";
      });
